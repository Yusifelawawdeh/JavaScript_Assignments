var randomNum = Math.floor(Math.random() * 6 ) + 1;
var guess = prompt("I am thinking of a number between 1 and 6. care to guess?");
if (parseInt(guess) === randomNum){
document.write("Congrats " + randomNum + "i s the right answer !");
} else {
document.write("sorry the correct number was " + randomNum);
}